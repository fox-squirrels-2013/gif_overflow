<div class="container">
  <div id="index_view">
  <h2>Welcome to GIFS</h1>
  </div>
</div>

<% if current_user %>
  </div>
    <div class="col-lg-4">
      <div class="well well-sm">
          <h3>Submit your own</h3>
          <p><%= render 'form' %></p>
      </div>
    </div>
  </div>
  </div>
<% end %>

<div class="col-lg-4">
  <div class="well well-lg">
    <h3>GIF Overload</h3>
    <ul class="index_gifs">
      <% @gifs.each do |gif| %>

      <%= render partial: 'postgif', locals: {gif: gif} %>

      <% end %>
    </ul>
  </div>
</div>






